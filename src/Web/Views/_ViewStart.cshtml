@{
    if(this.User.Identity.IsAuthenticated)
    {
        if(this.User.IsInRole("SuperAdmin"))
        {
            Layout = "/Views/Shared/_LayoutSUperAdmin.cshtml";
        }
        else
        {
            Layout = "/Views/Shared/_Layout.cshtml";
        }
    }
    else    
    {
        Layout = "/Views/Shared/_LayoutGuest.cshtml";
    }
}
